//Con este comado vas a poder Instaalr la carpeta Vendo de cualquier sistema Laravel solo cambiar por tu Usuario


1- usuario='alexis' && proy='' && dir='/var/www/'$proy && sudo chown -R $usuario:$usuario $dir && setfacl -R -m www-data:rwx $dir && setfacl -R -d -m www-data:rwx $dir && setfacl -R -m $usuario:rwx $dir && setfacl -R -d -m $usuario:rwx $dir


2- path='www' && dir='/var/'$path && user='alexis' && group='www-data' && sudo setfacl -R -m $group:rwx $dir && sudo setfacl -R -d -m $group:rwx $dir



